#include <iostream>

using namespace std;

int main() {

        int a, b, c;
        cout << "greq tver :"<<endl;
        cin >> a >> b >> c;
        for (int i = a; i > 0; i--) {
                if (a % i == 0 && b % i == 0 && c % i == 0) {
                        cout << "nod = " << i <<endl;
                        break;
                }
        }
        return 0;
}
 
